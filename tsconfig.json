{
  "compilerOptions": {
    "resolveJsonModule": true,
    "moduleResolution": "nodenext",
    "baseUrl": "./",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@helpers/*": ["./src/helpers/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@routes/*": ["./src/routes/*"],
      "@services/*": ["./src/services/*"],
      "@stores/*": ["./src/stores/*"],
      "@styles/*": ["./src/styles/*"],
      "@models/*": ["./src/models/*"],
      "@utils/*": ["./src/utils/*"]
    }
  },
  "extends": "@react-native/typescript-config/tsconfig.json",
  "exclude": ["node_modules"]
}
